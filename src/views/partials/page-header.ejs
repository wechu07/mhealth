<div class="container-fluid page-header mb-5 wow fadeIn" data-wow-delay="0.1s">
    <video autoplay loop muted class="custom-video">
        <source src="video/banner6.mp4" type="video/mp4">
        Your browser does not support the video tag.
    </video>
    <div class="video-overlay"></div>
    <div class="container text-center">
        <h1 class="display-4 text-white animated slideInDown mb-4">  <%= page %> </h1>
        <nav aria-label="breadcrumb animated slideInDown">
            <ol class="breadcrumb justify-content-center mb-0">
                <li class="breadcrumb-item"><a class="text-white" href="/">Back Home</a></li>
                <li class="breadcrumb-item text-primary active" aria-current="page"> <%= page %> </li>
            </ol>
        </nav>
    </div>
</div>

<!-- Page Header End -->